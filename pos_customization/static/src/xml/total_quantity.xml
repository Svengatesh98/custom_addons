<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <t t-name="pos_customization.OrderWidget" t-inherit="point_of_sale.OrderWidget" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('order-summary')]" position="inside">
            <div class="line">
                <div class="subentry fs-6 text-muted">
                           <strong>Total Quantity:</strong>
                    <span class="value">
                        <t t-esc="TotalQuantity"/>
                    </span>
                </div>
            </div>
        </xpath>
    </t>
</odoo>
