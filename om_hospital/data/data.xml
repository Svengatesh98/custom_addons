<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="seq_hospital_prescription" model="ir.sequence">
        <field name="name">Hospital Prescription Sequence</field>
        <field name="code">hospital.prescription</field>
        <field name="prefix">PRES-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

     <record id="seq_hospital_appointment_name" model="ir.sequence">
        <field name="name">hospital appointment Sequence</field>
        <field name="code">hospital.appointment</field>
        <field name="prefix">A-</field>
        <field name="padding">5</field>
       <field name="company_id" eval="False"/>
    </record>
