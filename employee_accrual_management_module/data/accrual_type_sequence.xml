<odoo>
    <data noupdate="1">
        <record id="seq_accrual_type_annual_leave" model="ir.sequence">
            <field name="name">Annual Leave Approved Reference</field>
            <field name="code">accrual.type.annual.leave</field>
            <field name="prefix">AL-</field> <!-- Change prefix as needed -->
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <record id="seq_accrual_type_annual_tickets" model="ir.sequence">
            <field name="name">Annual Tickets Approved Reference</field>
